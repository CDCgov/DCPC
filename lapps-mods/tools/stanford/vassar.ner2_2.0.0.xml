<tool id="stanford.ner2_2.0.0" name="Selectable Stanford NER v2.1.0" version="1.0.0">
 <description>
  Selectable Stanford Named Entity Recognizer (Vassar)
 </description>
 <command interpreter="lsd">
  invoke_ner2.lsd $model $input $output
 </command>
 <inputs>
  <param format="lif" label="input" name="input" type="data"/>
  <param label="Model" name="model" type="select">
   <option value="english.all.3class.distsim.crf.ser.gz">
    All 3
   </option>
   <option value="english.conll.4class.distsim.crf.ser.gz">
    CONLL 4
   </option>
   <option value="english.muc.7class.distsim.crf.ser.gz">
    MUC 7
   </option>
   <option value="english.nowiki.3class.distsim.crf.ser.gz">
    Nowiki 3
   </option>
  </param>
 </inputs>
 <outputs>
  <data format="lif" name="output">
  </data>
 </outputs>
 <help>
